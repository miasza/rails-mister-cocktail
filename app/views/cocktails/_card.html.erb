 <div class="container">
    <div class="row">

    <% @cocktails.each do |cocktail| %>
      <div class="col-md-6 col-lg-4 col-xl-3">
        <div class="card-trip">
          <img src="https://images.pexels.com/photos/997727/pexels-photo-997727.jpeg?auto=compress&cs=tinysrgb&dpr=1&w=500">
          <div class="card-trip-infos">
            <div>
              <h2><%= link_to cocktail.name, cocktail_path(cocktail) %>
              </h2>
            </div>
          </div>
        </div>


      </div>
    <% end %>
  </div>
